{
  "name": "scoretrackr",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "NODE_ENV=development next",
    "build": "next build",
    "start": "next start",
    "prisma": "prisma",
    "db:migrate:save": "prisma migrate save --experimental",
    "db:migrate": "prisma migrate up --experimental",
    "db:studio": "prisma studio --experimental",
    "format": "prettier --write . '**/*.{json,md,ts,tsx}'",
    "format:write": "prettier --write . '**/*.{json,md,ts,tsx}'",
    "format:check": "prettier --check . '**/*.{json,md,ts,tsx}'",
    "lint": "tslint -c tslint.json --project . --format verbose",
    "lint:fix": "tslint -c tslint.json --project . --fix --format verbose",
    "test": "TZ=UTC jest",
    "test:unit:ci": "TZ=UTC jest --watchAll=false --coverage",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@prisma/client": "^2.3.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@sendgrid/mail": "^7.2.1",
    "@welldone-software/why-did-you-render": "^4.2.6",
    "argon2": "^0.26.2",
    "date-fns": "^2.14.0",
    "i18next": "^19.6.2",
    "mdi-material-ui": "^6.16.0",
    "next": "^9.5.0",
    "next-auth": "^3.0.0",
    "next-i18next": "^5.1.0",
    "numbro": "^2.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "swr": "^0.2.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.5.0",
    "@prisma/cli": "^2.3.0",
    "@testing-library/dom": "^7.21.0",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^26.0.4",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "redux-devtools-extension": "^2.13.8",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "3.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
