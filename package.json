{
  "name": "scoretrackr",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "NODE_ENV=development next",
    "build": "next build",
    "start": "next start",
    "start:api-server": "ts-node --project tsconfig.api-server.json -r tsconfig-paths/register src/api-server/index.ts",
    "db:seed": "ts-node --project tsconfig.api-server.json node_modules/.bin/fixtures -cn default -c ./ormconfig.js ./src/api-server/db/fixtures",
    "db:drop": "NODE_PATH=./src ts-node --project tsconfig.api-server.json -r tsconfig-paths/register src/api-server/db/drop-database.ts",
    "format": "prettier --write . '**/*.{json,md,ts,tsx}'",
    "format:write": "prettier --write . '**/*.{json,md,ts,tsx}'",
    "format:check": "prettier --check . '**/*.{json,md,ts,tsx}'",
    "test": "TZ=UTC jest",
    "test:unit:ci": "TZ=UTC jest --watchAll=false --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reduxjs/toolkit": "^1.4.0",
    "@sendgrid/mail": "^7.2.1",
    "@welldone-software/why-did-you-render": "^4.2.6",
    "apollo-server-express": "^2.15.1",
    "body-parser": "^1.19.0",
    "date-fns": "^2.14.0",
    "express": "^4.17.1",
    "graphql": "^15.1.0",
    "graphql-hooks": "^4.5.0",
    "graphql-hooks-memcache": "^1.3.3",
    "i18next": "^19.6.2",
    "mdi-material-ui": "^6.16.0",
    "next": "latest",
    "next-i18next": "^5.0.0-beta.4",
    "numbro": "^2.3.1",
    "pg": "^8.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "type-graphql": "^1.0.0-rc.3",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.21.0",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.11",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "class-validator": "^0.12.2",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "pg-connection-string": "^2.3.0",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "redux-devtools-extension": "^2.13.8",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm-fixtures-cli": "^1.7.0",
    "typescript": "3.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
