{
  "npmScope": "scoretrackr",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "web": {
      "tags": []
    },
    "web-e2e": {
      "tags": [],
      "implicitDependencies": ["web"]
    },
    "utils": {
      "tags": ["scope:shared", "type:util"]
    },
    "ui-shared": {
      "tags": ["type:ui"]
    },
    "data-access-gql-queries": {
      "tags": ["type:data-access"]
    },
    "data-access-common-models": {
      "tags": ["type:data-access"]
    },
    "data-access-entities": {
      "tags": ["type:data-access"]
    }
  }
}
